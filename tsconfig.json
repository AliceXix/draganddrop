{
    "compilerOptions": {
        "incremental": true,
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "lib": [
            "esnext",
            "dom"
        ],
        "strict": true,
        "experimentalDecorators": true,
        "noUnusedLocals": true,
        "jsx": "react-jsx",
        "forceConsistentCasingInFileNames": false,
        "declaration": false,
        "allowSyntheticDefaultImports": true
    }
}